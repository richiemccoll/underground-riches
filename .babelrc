{
  "presets": [
    [
      "es2015",
      {
        "es2015": {
          "loose": true,
          "modules": false
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-async-to-generator",
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}